{
  "version": 2,
  "public": true,
  "cleanUrls": true,
  "trailingSlash": false,
  "routes": [
    {
      "src": "/static/(.*)",
      "headers": { "cache-control": "public, max-age=31536000, immutable" },
      "dest": "/static/$1"
    },
    {
      "src": "/(.*\\.(?:css|js|png|jpg|jpeg|webp|svg|gif|ico))",
      "headers": { "cache-control": "public, max-age=31536000, immutable" },
      "dest": "/$1"
    },
    {
      "src": "/(.*\\.html)",
      "headers": { "cache-control": "public, max-age=0, must-revalidate" },
      "dest": "/$1"
    },
    { "src": "/blogs", "headers": { "cache-control": "public, max-age=0, must-revalidate" }, "dest": "/blogs/index.html" },
    { "src": "/blogs/", "headers": { "cache-control": "public, max-age=0, must-revalidate" }, "dest": "/blogs/index.html" },
    { "src": "/", "headers": { "cache-control": "public, max-age=0, must-revalidate" }, "dest": "/index.html" }
  ]
}
